{% extends "base.html" %}
{{page_title}}
{{page_css}}
{% block content %}
    {% if login_form %}
        <form action="/kotw/login/" method="POST">{% csrf_token %}{{login_form}}<input type="submit" value="Login"></form>
        {{page_content|safe}}
    {% endif %}
    {% if register_form %}
        <form action="/kotw/register/" method="POST">{% csrf_token %}{{register_form}}<input type="submit" value="Register"></form>
        {{page_content|safe}}
    {% endif %}
{% endblock %}

