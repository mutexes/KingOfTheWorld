body {
	margin-left:auto;
	margin-right:auto;
	width:800px;
	background-image:url('kotw.jpg');
	background-repeat:repeat;
	background-position:left top;
	background-attachment:fixed; 
}
ul{
	list-style:none;
}
#column{
	position:relative;
	height:100%;
	width:100%;
	background-color:#888888;
	opacity:0.8;
	padding:25px;
}
#navbar{
	float:left;
	padding-left: 10px;
	padding-top: 10px;
	position: relative;
	height: 100%;
	width: 200px;
	max-height: 90%;
}
a:link {
	background-color:#888888;
	color: #000000;
	font-variant: small-caps;
}
a:visited {
	background-color:#888888;
	color: #000000;
	font-variant: small-caps;
}
a:hover {
	background-color:transparent;
	color: #000000;
	font-style:italic;
	font-variant: small-caps;
}
a:active {
	background-color:#888888;
	color: #000000;
	font-variant: small-caps;
}
p{
	text-align:center;
}
